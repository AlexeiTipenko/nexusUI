<!DOCTYPE html>
<html>
<head>
  <title>NexusOverview</title>
  <%= stylesheet_link_tag 'nexus.css' %>
  <%= javascript_include_tag :defaults %>
	<%= javascript_include_tag 'nexusUI/nexusDial.js'%>
	<%= javascript_include_tag 'nexusUI/nexusUI.js'%>
  <%= csrf_meta_tag %>

	<script type="text/javascript" charset="utf-8">
		function nexusInit(){
			dial1 = new dial("dial_1", 1, "example_send");
			dial2 = new dial("dial_2", 2, "example_send");
			// dial1.circle_size = 20.;
			// dial1.redraw();
		}
		
		function blockMove(event) {
		  // Tell Safari not to move the window.
		  event.preventDefault() ;
		 }
		
	</script>
</head>
<body ontouchmove="blockMove(event);" onLoad="nexusInit()">

<%= yield %>

</body>
</html>
